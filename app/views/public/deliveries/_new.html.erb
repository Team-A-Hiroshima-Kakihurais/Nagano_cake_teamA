   <div class="container">
    <div class="row">
     <div class="col">
     <%= form_with model: delivery do |f| %>
       <div class="form-group ml-4">
         <%= f.label :post_code, '郵便番号(ハイフンなし)' %>
         <%= f.text_field :post_code, class:"from-control ml-3" %>
       </div>
       <div class="form-group ml-4">
         <%= f.label :address, '住所' %>
         <%= f.text_field :address, class:"from-control ml-3" %>
       </div>
       <div class="form-group ml-4">
         <%= f.label :name, '宛名' %>
         <%= f.text_field :name, class:"from-control ml-3" %>
       </div>
       <%= f.submit '新規登録', class: 'btn btn-success btn-sm' %>
     <% end %>
     </div>
    </div>
  </div>

